{
    "_comment": "RA-D-PS Configuration Template - Copy this file to config.json and modify as needed",
    "batch_processing": {
        "batch_size": 50,
        "max_memory_mb": 1000,
        "parallel_processing": true,
        "max_workers": 4
    },
    "database": {
        "enable_individual_tracking": false,
        "database_pool_size": 5,
        "enable_database_backup": true
    },
    "validation": {
        "min_confidence_threshold": 1.0,
        "max_confidence_threshold": 5.0,
        "min_subtlety_threshold": 1.0,
        "max_subtlety_threshold": 5.0,
        "min_obscuration_threshold": 1.0,
        "max_obscuration_threshold": 5.0
    },
    "export": {
        "default_sheet_name": "radiology_data",
        "excel_blue_color": "FFCCE5FF",
        "auto_open_files": true,
        "include_metadata": true
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "ra_d_ps.log"
    }
}